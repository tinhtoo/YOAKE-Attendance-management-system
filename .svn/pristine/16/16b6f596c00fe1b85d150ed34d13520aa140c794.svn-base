/*----------------------------------------*/


/* container */


/*----------------------------------------*/

table#container {
    width: 100%;
}

div#container-top {
    padding: 0px;
    height: 82px;
}

div#container-bottom {
    padding: 0;
}


/*----------------------------------------*/


/* header */


/*----------------------------------------*/

div#header {
    position: relative;
    width: 100%;
    /* height: 100%; */
    background: url('../images/WorkTmMngMenuSecondOn-bg.gif') repeat-x 0px bottom;
}

div#header-inner {
    height: 65px;
    background: url('../images/WorkTmMngMenuSecondOn-bg.gif') repeat-y right bottom;
    text-align: left;
}

div#header table {
    width: 100%;
    height: 100%;
}

div#header td.logo {
    width: 100px;
    text-align: center;
    vertical-align: middle;
}

div#header td.title {
    vertical-align: middle;
    text-align: left;
    padding: 0;
}

div#header #header-welcom {
    padding: 10px;
    text-align: right!important;
}


/*----------------------------------------*/


/* body */


/*----------------------------------------*/

table#body {
    width: 100%;
    height: 100%;
    margin: 0;
    padding: 0;
}


/*----------------------------------------*/


/* menu */


/*----------------------------------------*/

td#menu {
    width: 190px;
    text-align: left;
    vertical-align: top;
    border: 1px solid #555;
    background: #fff;
}


/*----------------------------------------*/


/* body-border */


/*----------------------------------------*/

#vertical-splitter {
    border-top: 1px solid #555;
    border-bottom: 1px solid #555;
    border-left: 1px solid #555;
    background: url('../images/vertical-splitter-bg.gif') repeat-y 0px 0px;
    text-align: center;
    vertical-align: middle;
    cursor: pointer;
}

#vertical-splitter.on {
    background: url('../images/vertical-splitter-bg.gif') repeat-y -20px 0px;
}

#vertical-splitter p {
    height: 100px;
    width: 7px;
    padding: 0;
    margin: 0;
    background: url('../images/vertical-splitter-bg.gif') no-repeat -40px 0px;
}

#vertical-splitter p.on {
    background: url('../images/vertical-splitter-bg.gif') no-repeat -60px 0px;
}


/*----------------------------------------*/


/* body-border */


/*----------------------------------------*/

td.body-border {
    width: 7px;
}


/*----------------------------------------*/


/* contents */


/*----------------------------------------*/

td#contents {
    vertical-align: top;
    text-align: left;
    background: #fff;
    border: 1px solid #555;
}


/* contents-container */

td#contents div#contents-container {
    width: 100%;
    height: 100%;
}


/* contents-sitemap */

td#contents div#contents-container div#contents-sitemap {
    padding: 6px;
    background: #AED7FF url('../images/contents-sitemap-bg.gif');
    border-bottom: 1px solid #ddd;
}

td#contents div#contents-stage {
    padding: 20px;
}


/*----------------------------------------*/


/* CssClass */


/*----------------------------------------*/

.signout {
    text-decoration: none;
    font-size: 0.9em;
}