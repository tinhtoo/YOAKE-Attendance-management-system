            if( data[0].IN1_CNT >= 2 && !in1Time ){
                this.find($('.in1Time')).css('background-color','lightskyblue');
            }else if( out1Time && !in1Time ){
                this.find($('.in1Time')).css('background-color','tomato');
            }else{
                this.find($('.in1Time')).css('background-color','');
