<?php

namespace App\Repositories;

use Illuminate\Http\Request;
use App\Models\MT05Workptn;

class MT05WorkptnRepository
{    
    // 勤務体系から一般のみ取得 
    public function workptnsNormal()
    {
        return MT05Workptn::where('COM_CLS_CD', '01')
                ->orderby('WORKPTN_CD')
                ->get();
    }

}
